{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program via ts-node",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "program": "${workspaceFolder}/src/app.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true
        }
    ]
}
